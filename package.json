{
  "name": "gulp-dev-stack",
  "version": "1.0.0",
  "description": "Basic Gulp DevStack for basic front-end development with customized Bootstrap.",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js default",
    "develop": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js default",
    "commit": "git-cz",
    "csslint": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js csslint",
    "cssfix": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js cssfix",
    "format": "prettier --write **/*.{js,md,scss}",
    "gulp": "./node_modules/.bin/gulp",
    "html": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js html",
    "dataset": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js dataset",
    "rev": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js revision",
    "stylelint-find-unused-rules": "stylelint-find-rules",
    "css:lint": "stylelint **/*.scss",
    "css:fix": "stylelint --fix **/*.scss",
    "js:lint": "eslint **/*.js",
    "js:fix": "eslint --fix **/*.js",
    "text:lint": "remark -q .",
    "text:fix": "remark -q . -o --",
    "release": "release-it",
    "version": "auto-changelog -p -t keepachangelog.hbs && npm run text:fix && git add CHANGELOG.md"
  },
  "dependencies": {
    "bootstrap": "4.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@release-it/conventional-changelog": "^1.1.4",
    "@visionappscz/stylelint-config-visionapps": "^1.1.0",
    "@visionappscz/stylelint-config-visionapps-order": "^1.1.0",
    "auto-changelog": "^2.2.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.12",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-favicons": "^2.4.0",
    "gulp-google-webfonts": "^4.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-jsbeautifier": "^3.0.1",
    "gulp-markdown-to-json": "^1.1.0",
    "gulp-merge-json": "^2.1.1",
    "gulp-newer": "^1.4.0",
    "gulp-nunjucks": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "gulp-rev-rewrite": "^3.0.2",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sri-hash": "^2.2.1",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-upng": "^1.0.3",
    "husky": "^4.2.3",
    "imagemin-mozjpeg": "^9.0.0",
    "lint-staged": "^10.2.11",
    "marked": "^1.1.1",
    "nunjucks-date-filter-locale": "^1.4.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-scss": "^2.1.1",
    "prettier": "^2.0.5",
    "release-it": "^13.6.6",
    "remark": "^12.0.1",
    "remark-cli": "^8.0.1",
    "remark-lint-match-punctuation": "^0.2.0",
    "remark-preset-lint-consistent": "^3.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "stylelint": "^13.6.1",
    "stylelint-find-rules": "^2.2.0",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": "https://github.com/cebreus/gulp-devstack",
  "homepage": "https://github.com/cebreus/gulp-devstack#readme",
  "bugs": {
    "url": "https://github.com/cebreus/gulp-devstack/issues",
    "email": "cebreus@live.com"
  },
  "author": "Jaroslav Vr√°na <cebreus@live.com>",
  "private": false,
  "license": "MIT",
  "keywords": [
    "devstack",
    "gulp",
    "bootstrap"
  ],
  "lint-staged": {
    "--allow-empty *.{css,scss,md,js,jsx,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ],
    "--allow-empty *.js": [
      "eslint --fix",
      "git add"
    ],
    "--allow-empty *.md": [
      "npm run text:fix",
      "git add"
    ],
    "--allow-empty *.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
