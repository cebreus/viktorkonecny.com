{
  "name": "viktorkonecny",
  "version": "1.0.2",
  "description": "Persoval site of the Viktor Konečný",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js default",
    "dev": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js default",
    "deploy": "netlify build && netlify deploy --prod",
    "commit": "git-cz",
    "format": "prettier --write **/*.{css,scss,md,js,jsx,ts,tsx,json} && npm run js:fix && npm run css:fix && npm run text:fix",
    "css:lint": "node ./node_modules/stylelint/bin/stylelint.js **/*.scss",
    "css:fix": "node ./node_modules/stylelint/bin/stylelint.js --fix **/*.scss",
    "css:unused": "node ./node_modules/stylelint-find-rules/src/bin/stylelint-find-rules.js",
    "js:lint": "node ./node_modules/eslint/bin/eslint.js **/*.js",
    "js:fix": "node ./node_modules/eslint/bin/eslint.js --fix **/*.js",
    "text:lint": "node ./node_modules/remark-cli/cli.js -q .",
    "text:fix": "node ./node_modules/remark-cli/cli -q . -o --",
    "release": "release-it",
    "version": "node ./node_modules/auto-changelog/src/index.js -p -t keepachangelog.hbs && git add CHANGELOG.md"
  },
  "dependencies": {
    "@netlify/plugin-sitemap": "^0.5.1",
    "bootstrap": "^4.5.2",
    "netlify-plugin-checklinks": "^4.1.1",
    "netlify-plugin-no-more-404": "0.0.15"
  },
  "devDependencies": {
    "gulp-brotli": "^3.0.0",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@visionappscz/stylelint-config-visionapps": "^1.1.0",
    "@visionappscz/stylelint-config-visionapps-order": "^1.1.0",
    "auto-changelog": "^2.2.1",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.12",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-favicons": "^2.4.0",
    "gulp-google-webfonts": "^4.0.0",
    "gulp-html-beautify": "^1.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-jsbeautifier": "^3.0.1",
    "gulp-markdown-to-json": "^1.1.0",
    "gulp-merge-json": "^2.1.1",
    "gulp-newer": "^1.4.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-purgecss": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-all": "^2.0.3",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "gulp-rev-rewrite": "^1.1.4",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sri-hash": "^2.2.1",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-upng": "^1.0.3",
    "husky": "^4.3.0",
    "imagemin-mozjpeg": "^9.0.0",
    "lint-staged": "^10.4.0",
    "marked": "^1.2.0",
    "nunjucks-date-filter-locale": "^1.4.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-scss": "^3.0.1",
    "prettier": "^2.1.2",
    "release-it": "^14.0.3",
    "remark": "^12.0.1",
    "remark-cli": "^8.0.1",
    "remark-frontmatter": "^2.0.0",
    "remark-lint-match-punctuation": "^0.2.0",
    "remark-preset-lint-consistent": "^3.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "stylelint": "^13.7.2",
    "stylelint-find-rules": "^2.2.0",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": "https://github.com/cebreus/viktorkonecny.com",
  "homepage": "https://github.com/cebreus/viktorkonecny.com#readme",
  "author": "Jaroslav Vrána <cebreus@live.com>",
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npx lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{css,scss,md,js,jsx,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --cache --fix"
    ],
    "*.md": [
      "npm run text:fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
