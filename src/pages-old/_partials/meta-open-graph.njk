<meta name="--og" content="{{ open_graph | dump }}">


{% if open_graph.use and open_graph.use === true %}
	{% if open_graph.title and open_graph.title | length  %}
		<meta property="og:title" content="{{ open_graph.title or title }}">
	{% endif %}
	{% if open_graph.description and open_graph.description | length  %}
		<meta property="og:description" content="{{ open_graph.description }}">
	{% endif %}
	{% if open_graph.image and open_graph.image | length  %}
		<meta property="og:image" content="{{ open_graph.image }}">
	{% endif %}
	{% if open_graph.type %}
		<meta property="og:type" content="{{ open_graph.type }}">
	{% endif %}
	{# <meta property="og:url" content="{{ SITE.meta.canonical }}"> #}
	{% if SITE.open_graph.app_id %}
		<meta property="fb:app_id" content="{{ SITE.open_graph.app_id }}">
	{% endif %}
	{% if SITE.open_graph.site_name %}
		<meta property="og:site_name" content="{{ SITE.open_graph.site_name }}">
	{% endif %}
{% elif BLOGLIST.open_graph.use and BLOGLIST.open_graph.use === true %}
	{% if BLOGLIST.open_graph.title and BLOGLIST.open_graph.title | length  %}
		<meta property="og:title" content="{{ BLOGLIST.open_graph.title or BLOGLIST.title }}">
	{% endif %}
	{% if BLOGLIST.open_graph.description and BLOGLIST.open_graph.description | length  %}
		<meta property="og:description" content="{{ BLOGLIST.open_graph.description }}">
	{% endif %}
	{% if BLOGLIST.open_graph.image and BLOGLIST.open_graph.image | length  %}
		<meta property="og:image" content="{{ BLOGLIST.open_graph.image }}">
	{% endif %}
	{% if BLOGLIST.open_graph.type %}
		<meta property="og:type" content="{{ BLOGLIST.open_graph.type }}">
	{% endif %}
	{# <meta property="og:url" content="{{ SITE.meta.canonical }}"> #}
	{% if SITE.open_graph.app_id %}
		<meta property="fb:app_id" content="{{ SITE.open_graph.app_id }}">
	{% endif %}
	{% if SITE.open_graph.site_name %}
		<meta property="og:site_name" content="{{ SITE.open_graph.site_name }}">
	{% endif %}
{% elif SITE.open_graph.use and SITE.open_graph.use === true %}
	{% if SITE.open_graph.title   %}
